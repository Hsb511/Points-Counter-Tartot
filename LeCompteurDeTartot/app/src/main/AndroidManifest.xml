<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="team23.lecompteurdetartot">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_tartot"
        android:label="@string/label_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PartyCreationActivity" />
        <activity android:name=".PartyActivity" />
        <activity android:name=".PlayerActivity" />
        <activity android:name=".PartyListActivity"></activity>
    </application>

</manifest>